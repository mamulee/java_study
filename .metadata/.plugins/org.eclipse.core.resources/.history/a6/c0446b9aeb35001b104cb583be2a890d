package p09;

public class DataBox {
	private String data;
	public synchronized String getData() {
		if(this.data == null) {
			try {
				wait();
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
			String returnValue = data;
			System.out.println("ConsummerThread가 읽은 데이터: "+ returnValue);
		}
	}
}
